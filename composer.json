{
    "name": "fwkit/teddy-sample",
    "description": "Teddy sample app",
    "type": "project",
    "require": {
        "fwkit/teddy": "dev-master",
        "mix/guzzle-hook": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "extra": {
        "include_files": [
            "vendor/mix/guzzle-hook/src/functions_include.php"
        ]
    },
    "authors": [
        {
            "name": "Fung Wing Kit",
            "email": "wengee@gmail.com"
        }
    ],
    "scripts": {
        "build": "composer dump-autoload -a && php build.php && composer dump-autoload"
    }
}
